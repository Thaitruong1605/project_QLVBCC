<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Nhập chứng chỉ mới</title>
  <%- include('../../layouts/styles'); %>
</head>

<body>
  <div class="container">
    <div class="row">
      <table class="table">
        <thead>
          <tr>
            <th scope="col">header</th>
            <th scope="col">info</th>
          </tr>
        </thead>
        <tbody>
          <% Object.keys(cert_info).forEach(function(key) { %>
            <tr>
              <td><%= key %></td>
              <td><%= cert_info[key] %></td>
            </tr>
         <% }); %>
        </tbody>
      </table>
    </div>
  </div>
  <%- include('../../layouts/scripts.ejs'); %>
</body>

</html>